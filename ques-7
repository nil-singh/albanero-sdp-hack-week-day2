#include <iostream>
using namespace std;


int main(){
    cout<<"Enter the number of rows in an array "<<endl;
    int n;
    cin>>n;
    cout<<"Enter the number of columns in an array "<<endl;
    int m;
    cin>>m;

    cout<<"Enter the values of each element in an array "<<endl;
    int arr[n][m];
    for(int i=0; i<n; i++){
        for(int j=0; j<m; j++){
            cin>>arr[i][j];
        }
    }

    int sum=0;
    for(int i=0; i<n; i++){
        for(int j=0; j<m; j++){
            sum=sum+arr[i][j];
        }
    }
    cout<<"The sum of elements in a given 2D array is "<<sum<<endl;

    return 0;

}
